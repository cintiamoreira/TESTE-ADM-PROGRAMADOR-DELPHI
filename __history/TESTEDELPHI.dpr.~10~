program TESTEDELPHI;

uses
  Vcl.Forms,
  MenuPrincipal in 'MenuPrincipal.pas' {frmPrincipal},
  uDTMConexao in 'datamodule\uDTMConexao.pas' {dtmPrincipal: TDataModule},
  TelaConfiguracao in 'TelaConfiguracao.pas' {frmTelaConfiguracao},
  TelaHerancaCadastros in 'TelaHerancaCadastros.pas' {frmTelaHerancaCadastros},
  TelaCadastrosProdutos in 'TelaCadastrosProdutos.pas' {frmTelaHerancaCadastros1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmPrincipal, frmPrincipal);
  Application.CreateForm(TfrmTelaConfiguracao, frmTelaConfiguracao);
  Application.CreateForm(TfrmTelaHerancaCadastros, frmTelaHerancaCadastros);
  Application.CreateForm(TfrmTelaHerancaCadastros1, frmTelaHerancaCadastros1);
  Application.Run;
end.
